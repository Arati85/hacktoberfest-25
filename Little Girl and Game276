#include<bits/stdc++.h>
using namespace std;
string solve(string s){
    int count=0;
    unordered_map<char,int>mp;
    for(int k=0;k<s.size();k++){
        mp[s[k]]++;
    }
    for(auto it:mp){
        if(it.second %2!=0){
            count++;
        }
    }
   if (count <= 1) 
        return "First";
    else 
        return (count % 2 == 1) ? "First" : "Second";
   
}
